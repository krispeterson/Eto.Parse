{
	"version": "1.4.1",
	"buildOptions": {
		"compile": {
			"include": [
				"../Eto.Parse.Samples/Markdown/**.cs"
			]
		}
	},
	"dependencies": {
		"Eto.Parse": "1.4.1",
		"xunit": "2.2.0-beta2-build3300",
		"dotnet-test-xunit": "2.2.0-preview2-build1029"
	},
	"frameworks": {
		"net451": {
			"frameworkAssemblies": {
				"System.Runtime": {
					"type": "build"
				},
				"System.Threading.Tasks": {
					"type": "build"
				}
			}
		},
		"netcoreapp1.0": {
			"dependencies": {
				"Microsoft.NETCore.App": {
					"type": "platform",
					"version": "1.0.0"
				}
			},
			"buildOptions": {
				"define": [
					"CORECLR"
				],
				"debugType": "portable"
			}
		}
	},
	"testRunner": "xunit"
}
